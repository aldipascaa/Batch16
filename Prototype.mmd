
classDiagram
    direction LR

    class IPrototype~T~ {
      +T Clone()
    }

    class RequestHeader {
      + string Key
      + string Value
    }

    class HttpRequestTemplate {
      + string BaseUrl
      + string Path
      + string Method
      + List~RequestHeader~ Headers
      + string AuthToken
      + int TimeoutMs
      + HttpRequestTemplate Clone()
    }

    class ApiClient {
      + void Send(HttpRequestTemplate template, string pathOverride, string? body)
    }

    IPrototype <|.. HttpRequestTemplate : implements
    HttpRequestTemplate --> "0..*" RequestHeader : contains
    ApiClient ..> HttpRequestTemplate : uses Clone()
